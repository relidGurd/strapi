{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Товары",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subTitle": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "biginteger",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "date": {
      "type": "date",
      "unique": false
    },
    "quantity": {
      "type": "integer",
      "max": 20,
      "required": true,
      "default": 1
    }
  }
}
